    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "knex": "^0.21.2",
    "mime": "^2.4.6",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
