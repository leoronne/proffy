  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "node -r ./tsconfig-paths-bootstrap.js dist/server.js",
    "knex:migrate": "knex --knexfile src/knexfile.ts migrate:latest",
    "knex:migrate:undo": "knex --knexfile src/knexfile.ts migrate:rollback",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files"
  },
  "dependencies": {
